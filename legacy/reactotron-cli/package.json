{
  "name": "reactotron-cli",
  "version": "1.7.0",
  "description": "Reactotron terminal mode engage!",
  "bin": {
    "reactotron": "./dist/index.js"
  },
  "scripts": {
    "test": "ava",
    "watch": "ava --watch",
    "coverage": "nyc ava",
    "start": "babel-node src/index.js",
    "build": "rollup -c && mv dist/index.js dist/index.js.tmp && echo '#!/usr/bin/env node\n' > ./dist/index.js && cat dist/index.js.tmp >> ./dist/index.js && chmod +x ./dist/index.js",
    "lint": "standard"
  },
  "repository": "https://github.com/reactotron/reactotron/tree/master/packages/reactotron-cli",
  "author": "Steve Kellock",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reactotron/reactotron/issues"
  },
  "homepage": "https://github.com/reactotron/reactotron",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "ava": "^0.18.1",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-stage-1": "^6.22.0",
    "nyc": "^10.1.2",
    "rollup": "^0.41.4",
    "rollup-plugin-babel": "^2.6.1",
    "standard": "^8.0.0"
  },
  "ava": {
    "require": [
      "babel-core/register"
    ],
    "babel": "inherit"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "dependencies": {
    "blessed": "^0.1.81",
    "gemoji": "^2.0.1",
    "mobx": "^2.5.1",
    "moment": "^2.15.0",
    "ramda": "^0.23.0",
    "ramdasauce": "^1.2.0",
    "reactotron-core-server": "^1.7.0",
    "strman": "^1.0.1"
  }
}
