{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "os:default",
    "store:default",
    {
      "identifier": "opener:allow-open-url",
      "allow": [
        {
          "url": "https://*"
        }
      ]
    },
    "fs:default",
    "clipboard-manager:default"
  ]
}