{
  "name": "reactotron-apisauce",
  "version": "2.0.0-alpha.2",
  "description": "A Reactotron plugin for Apisauce.",
  "main": "dist/index.js",
  "scripts": {
    "test": "ava",
    "format": "prettier --write {**,.}/*.js && standard --fix",
    "watch": "ava --watch",
    "coverage": "nyc ava",
    "build": "rollup -c",
    "lint": "standard"
  },
  "repository": "https://github.com/reactotron/reactotron/tree/master/packages/reactotron-apisauce",
  "author": "Steve Kellock",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reactotron/reactotron/issues"
  },
  "homepage": "https://github.com/reactotron/reactotron",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.2.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-stage-1": "^6.24.1",
    "json-server": "^0.10.1",
    "mockery": "^2.0.0",
    "nyc": "^10.3.2",
    "prettier": "^1.10.2",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.6.1",
    "standard": "^10.0.3"
  },
  "ava": {
    "require": [
      "babel-core/register"
    ],
    "babel": {
      "babelrc": false,
      "presets": [
        "es2015",
        "stage-1"
      ]
    }
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "dependencies": {
    "apisauce": "^0.14.0",
    "ramda": "^0.23.0",
    "ramdasauce": "^1.2.0",
    "reactotron-core-client": "^2.0.0-alpha.2"
  }
}
