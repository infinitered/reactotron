{
  "name": "reactotron-apisauce",
  "version": "2.0.0-beta.8",
  "description": "A Reactotron plugin for Apisauce.",
  "main": "dist/index.js",
  "scripts": {
    "test": "ava",
    "format": "prettier --write {**,.}/*.js",
    "watch": "ava --watch",
    "coverage": "nyc ava",
    "build": "rollup -c",
    "prepare": "yarn build"
  },
  "repository": "https://github.com/reactotron/reactotron/tree/master/packages/reactotron-apisauce",
  "author": "Steve Kellock",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reactotron/reactotron/issues"
  },
  "homepage": "https://github.com/reactotron/reactotron",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-stage-1": "^6.24.1",
    "json-server": "^0.12.2",
    "mockery": "^2.1.0",
    "nyc": "^11.7.1",
    "prettier": "^1.12.1",
    "rollup": "^0.58.2",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-filesize": "^1.5.0"
  },
  "ava": {
    "require": [
      "babel-core/register"
    ],
    "babel": {
      "babelrc": false,
      "presets": [
        "es2015",
        "stage-1"
      ]
    }
  },
  "dependencies": {
    "apisauce": "^0.14.3",
    "reactotron-core-client": "^2.0.0-beta.8"
  }
}
