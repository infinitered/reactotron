{
  "name": "reactotron-core-client",
  "version": "1.5.2",
  "description": "Grants Reactotron clients the ability to talk to a Reactotron server.",
  "main": "dist/index.js",
  "scripts": {
    "test": "ava",
    "watch": "ava --watch",
    "coverage": "nyc ava",
    "build": "rollup -c",
    "send": "babel-node scripts/send.js",
    "lint": "standard"
  },
  "repository": "https://github.com/reactotron/reactotron/tree/master/packages/reactotron-core-client",
  "author": "Steve Kellock",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reactotron/reactotron/issues"
  },
  "homepage": "https://github.com/reactotron/reactotron",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^7.1.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2015-rollup": "^1.2.0",
    "babel-preset-stage-1": "^6.13.0",
    "nyc": "^8.1.0",
    "rollup": "^0.36.3",
    "rollup-plugin-babel": "^2.6.1",
    "standard": "^8.0.0"
  },
  "ava": {
    "require": [
      "babel-core/register"
    ],
    "babel": {
      "babelrc": false,
      "presets": [
        "es2015",
        "stage-1"
      ]
    }
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "dependencies": {
    "json-stringify-safe": "^5.0.1",
    "ramda": "^0.22.1",
    "ramdasauce": "^1.1.1",
    "socket.io": "^1.7.0"
  }
}
