{
  "name": "reactotron-server",
  "version": "2.0.0",
  "description": "A reactotron server",
  "main": "index.js",
  "scripts": {
    "compile": "tsc -p tsconfig.server.json",
    "clean": "rimraf dist build",
    "lint": "tslint -p .",
    "build:server": "yarn clean && tsc --p tsconfig.server.json",
    "start:server": "yarn build:server && node dist/server/index.js",
    "start:app": "yarn parcel watch src/app/index.html",
    "test": "npm-run-all test:compile && yarn ava",
    "test:watch": "npm-run-all test:compile && yarn ava --watch",
    "test:compile": "tsc -p tsconfig.test.json --pretty",
    "test:compile:watch": "tsc -p tsconfig.test.json --pretty --watch"
  },
  "keywords": [],
  "author": "https://github.com/infinitered/reactotron/graphs/contributors",
  "bugs": {
    "url": "https://github.com/infinitered/reactotron/issues"
  },
  "homepage": "https://github.com/infinitered/reactotron",
  "license": "MIT",
  "repository": "infinitered/reactotron",
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/react": "^16.4.13",
    "@types/react-dom": "^16.0.7",
    "ava": "^0.25.0",
    "graphql-request": "^1.8.2",
    "npm-run-all": "^4.1.3",
    "parcel-bundler": "^1.9.7",
    "prettier": "^1.14.2",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "request-promise-native": "^1.0.5",
    "rimraf": "^2.6.2",
    "testdouble": "^3.8.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.2.9",
    "apollo-client": "^2.4.1",
    "apollo-link": "^1.2.2",
    "apollo-link-error": "^1.1.0",
    "apollo-link-http": "^1.5.4",
    "apollo-link-ws": "^1.0.8",
    "apollo-server-express": "^2.0.4",
    "express": "^4.16.3",
    "fs-jetpack": "^2.1.0",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.9.2",
    "graphql-type-json": "^0.2.1",
    "react": "^16.5.0",
    "react-apollo": "^2.1.11",
    "react-dom": "^16.5.0",
    "reactotron-core-server": "^2.1.0",
    "reflect-metadata": "^0.1.12",
    "subscriptions-transport-ws": "^0.9.14",
    "type-graphql": "^0.13.1",
    "yargs-parser": "^10.1.0"
  },
  "ava": {
    "files": [
      "build/**/*.test.js"
    ]
  },
  "resolutions": {
    "graphql": "^14.0.2",
    "@types/graphql": "^14"
  }
}
