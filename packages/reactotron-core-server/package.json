{
  "name": "reactotron-core-server",
  "version": "1.12.0",
  "description": "Grants Reactotron servers the ability to talk to a Reactotron client.",
  "main": "dist/index.js",
  "scripts": {
    "test": "ava",
    "watch": "ava --watch",
    "coverage": "nyc ava",
    "build": "rollup -c",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "build:watch": "node watch.js"
  },
  "repository": "https://github.com/reactotron/reactotron/tree/master/packages/reactotron-core-server",
  "author": "Steve Kellock",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reactotron/reactotron/issues"
  },
  "homepage": "https://github.com/reactotron/reactotron",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@types/node": "^7.0.31",
    "@types/ramda": "^0.24.17",
    "ava": "^0.20.0",
    "execa": "^0.6.3",
    "node-watch": "^0.5.4",
    "nyc": "^11.0.3",
    "rollup": "^0.43.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-typescript2": "^0.4.3",
    "tslib": "^1.7.1",
    "tslint": "^5.5.0",
    "tslint-eslint-rules": "^4.1.1",
    "tslint-react": "^3.1.0",
    "tslint-react-recommended": "^1.0.9",
    "typescript": "^2.4.0"
  },
  "ava": {
    "require": [
      "babel-core/register"
    ],
    "babel": {
      "babelrc": false,
      "presets": [
        "es2015",
        "stage-1"
      ]
    }
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "dependencies": {
    "ramda": "^0.24.1",
    "ramdasauce": "^2.0.0",
    "ws": "^2.2.0"
  }
}
