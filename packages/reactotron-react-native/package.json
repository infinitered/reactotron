      "presets": [
        "es2015",
        "stage-1"
      ]
    }
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "dependencies": {
    "ramda": "^0.21.0",
    "ramdasauce": "^1.1.0",
    "react": "^15.2.1",
    "react-native": "^0.30.0",
    "reactotron-core-client": "^0.1.0",
    "socket.io": "^1.4.8"
  }
