{
  "name": "reactotron-monorepo",
  "version": "0.0.1",
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "clean": "lerna clean --yes",
    "build": "lerna run build --scope 'reactotron-core-*' --scope 'reactotron-redux*' --scope 'reactotron-apisauce' --scope 'reactotron-react-native' --scope 'reactotron-react-js'",
    "copy-internal-deps": "cd packages/demo-react-native && ./copy-internal-deps.sh && cd ../..",
    "test": "lerna run test --scope 'reactotron-core-*' --scope 'reactotron-redux*' --scope 'reactotron-apisauce' --scope 'reactotron-react-native' --scope 'reactotron-react-js'",
    "lint": "lerna run lint",
    "welcome": "yarn run clean && yarn run bootstrap -- --ignore-engines && yarn run build && yarn run copy-internal-deps && yarn run lint -s"
  },
  "devDependencies": {
    "lerna": "^2.0.0-rc.4"
  },
  "dependencies": {
    "stringify-object": "^3.2.0"
  }
}
