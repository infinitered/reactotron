  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "reactotron-react-js.d.ts"
  ],
  "main": "dist/index.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch --notify",
    "format": "prettier --write {**,.}/*.ts",
