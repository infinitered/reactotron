    "prettier": "^1.16.4",
    "reactotron-core-client": "^2.5.0-beta.2",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-babel-minify": "^7.0.0",
